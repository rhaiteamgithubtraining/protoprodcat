<mvc:View controllerName="nsNWPC_prjNWProdCatalog.controller.Master" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<Page id="idMasterPage" title="">
		<subHeader>
			<Bar>
				<contentLeft>
					<SearchField id="idSearchField"
								 search="handleidSearchFieldSearch"
								 width="100%"></SearchField>
				</contentLeft>
			</Bar>
		</subHeader>
		<List id="idList"
			  mode=" {device>/listMode}"
			  items="{appdata>/Products}"
			  noDataText="{i18n>errornodata}"
			  selectionChange="handleidlistselectionChange">
			<ObjectListItem title="{appdata>ProductName}"
							type="Active"
							number=""
							press="handleobjlistitemPress"
							type="{device>/listItemType}">
				<attributes>
					<ObjectAttribute text="{appdata>Category/CategoryName}"/>
					<ObjectAttribute text="{appdata>Supplier/CompanyName}"/>
				</attributes>
			</ObjectListItem>
		</List>
		<footer></footer>
	</Page>
</mvc:View>